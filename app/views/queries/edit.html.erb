<h3>Edit query</h3>
<p> <%= flash[:notice] %> </p>
<%= form_for @query do |f| %>
    <%= render :partial => "form", :object => f %>
    <div class="action-buttons">
      <%= f.submit "Update", :name => "update" %>
      <%= link_to f.submit("Cancel", :name => "cancel"), queries_url %>
    </div>
<% end %>
