<h3>Create new Query</h3>
<%= form_for @query do |f| %>
    <%= render :partial => "form", :object => f %>
    <div class="action-buttons">
      <%= f.submit "Create", :name => "create", :class=>'create-button' %>
      <%= link_to f.submit("Cancel", :name => "cancel", :class=>'cancel-button'), :controller=>:queries, :action=>:index %>
    </div>
<% end %>
